{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch Blocks",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Blocks/bin/Debug/net6.0/Blocks.dll",
            "cwd": "${workspaceFolder}/Blocks",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch Circle",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Circle/bin/Debug/net6.0/Circle.dll",
            "cwd": "${workspaceFolder}/Circle",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
